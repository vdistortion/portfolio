{
  "name": "vdistortion",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier . --write",
    "public": "ng build --base-href /vdistortion/ && cd dist/vdistortion/browser && cp index.html 404.html && gh-pages -d . -t --nojekyll",
    "to-webp": "npx to-webp-json@latest json=source width=1000 height=1000"
  },
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/youtube-player": "^20.0.2",
    "@mdi/js": "^7.4.47",
    "@tsparticles/angular": "^3.0.0",
    "@tsparticles/preset-links": "^3.2.0",
    "@twogate/ngx-photo-gallery": "^1.8.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jasmine": "~5.1.0",
    "gh-pages": "^6.3.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "3.5.3",
    "typescript": "~5.8.2"
  }
}
